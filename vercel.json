{ 
    "version": 2, 
    "builds":[{"src":"./index.js","use":"@vercel/node"}],
    "routes":[{
        "src":"/(.*)",
        "dest":"/",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }],
    
  "headers": [
    {
      "source": "/api/your-api-route",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "http://localhost:4200" 
        }
      ]
    }
  ]
}